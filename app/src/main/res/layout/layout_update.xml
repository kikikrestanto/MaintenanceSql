<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="horizontal"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/table_content_cell_bg"
    >
            <TextView
                android:id="@+id/nomorTextView"
                android:text="100"
                android:textSize="12dp"
                android:layout_gravity="center"
                android:layout_width="25dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:padding="10dp"
                />

            <TextView
                android:id="@+id/tanggalTextView"
                android:text="12/10/2020"
                android:textSize="12dp"
                android:layout_width="85dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:padding="10dp"
                />
            <TextView
                android:id="@+id/tanggalDuaView"
                android:text="12/10/2020"
                android:textSize="12dp"
                android:layout_width="85dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:padding="10dp"
                />
            <TextView
                android:id="@+id/tindakanTextView"
                android:text="Cuci Steam, Service Rutin"
                android:textSize="12dp"
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:padding="10dp"
                />
            <TextView
                android:id="@+id/keteranganTextView"
                android:textSize="12dp"
                android:text="Done"
                android:layout_width="50dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:padding="10dp"
                />

</LinearLayout>