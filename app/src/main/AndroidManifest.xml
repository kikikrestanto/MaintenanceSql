<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.maintenancesql">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".Activities.EditUserAct"></activity>
        <activity android:name=".Activities.UpdateAct" />
        <activity android:name=".Activities.DetailAct" />
        <activity android:name=".Activities.addPostAct" />
        <activity android:name=".Fragments.AccountFragment" />
        <activity android:name=".Fragments.HomeFragment" />
        <activity android:name=".Activities.UserInfoAct" />
        <activity android:name=".Activities.LoginAct" />
        <activity android:name=".Activities.HomeAct" />
        <activity android:name=".Activities.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>